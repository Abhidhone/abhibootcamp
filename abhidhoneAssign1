    "# Step 1: Create a string\n",
    "input_string = \"Hello world this is Python\"\n",
    "\n",
    "# Step 2: Print the string in reverse order\n",
    "reversed_string = input_string[::-1]\n",
    "print(\"Reversed string:\", reversed_string)\n",
    "\n",
    "# Step 3: Reverse the individual words only\n",
    "words = input_string.split()  # Split string into a list of words\n",
    "reversed_words = ' '.join(word[::-1] for word in words)  # Reverse each word and join them back\n",
    "print(\"String with reversed words:\", reversed_words)\n"
